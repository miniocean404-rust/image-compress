#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/soft-dev/code/rust/image-compress/node_modules/.pnpm/@napi-rs+cli@1.3.5/node_modules/@napi-rs/cli/scripts/node_modules:/mnt/d/soft-dev/code/rust/image-compress/node_modules/.pnpm/@napi-rs+cli@1.3.5/node_modules/@napi-rs/cli/node_modules:/mnt/d/soft-dev/code/rust/image-compress/node_modules/.pnpm/@napi-rs+cli@1.3.5/node_modules/@napi-rs/node_modules:/mnt/d/soft-dev/code/rust/image-compress/node_modules/.pnpm/@napi-rs+cli@1.3.5/node_modules:/mnt/d/soft-dev/code/rust/image-compress/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/soft-dev/code/rust/image-compress/node_modules/.pnpm/@napi-rs+cli@1.3.5/node_modules/@napi-rs/cli/scripts/node_modules:/mnt/d/soft-dev/code/rust/image-compress/node_modules/.pnpm/@napi-rs+cli@1.3.5/node_modules/@napi-rs/cli/node_modules:/mnt/d/soft-dev/code/rust/image-compress/node_modules/.pnpm/@napi-rs+cli@1.3.5/node_modules/@napi-rs/node_modules:/mnt/d/soft-dev/code/rust/image-compress/node_modules/.pnpm/@napi-rs+cli@1.3.5/node_modules:/mnt/d/soft-dev/code/rust/image-compress/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@napi-rs/cli/scripts/index.js" "$@"
else
  exec node  "$basedir/../@napi-rs/cli/scripts/index.js" "$@"
fi
