{
    "name": "@image-compress/workspace",
    "version": "1.0.0",
    "packageManager": "pnpm@9.6.0",
    "private": true,
    "repository": {
        "type": "git",
        "url": "https://github.com/miniocean404-rust/image-compress"
    },
    "scripts": {
        "changelog": "git cliff --output CHANGELOG.md",
        "build:node": "pnpm --filter=@giegie/* build:node",
        "build:wasm": "pnpm --filter=@giegie/* build:wasm",
        "publish:test": "pnpm --filter=@giegie/* publish --dry-run --no-git-checks"
    },
    "devDependencies": {
        "git-cliff": "^2.4.0"
    }
}
