{
  "package": "@@giegie/core",
  "license": "MIT",
  "version": "1.0.0",
  "napi": {
    "binaryName": "explore",
    "package": {
      "name": "explore"
    },
    "targets": [
      "x86_64-apple-darwin",
      "x86_64-pc-windows-msvc",
      "x86_64-unknown-linux-gnu",
      "x86_64-unknown-linux-musl",
      "i686-pc-windows-msvc",
      "armv7-unknown-linux-gnueabihf",
      "aarch64-unknown-linux-gnu",
      "aarch64-apple-darwin",
      "aarch64-unknown-linux-musl",
      "aarch64-pc-windows-msvc"
    ]
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "scripts": {
    "build:wasm-dev": "npm-run-all \"pack -- build ../crates/wasm --dev --out-dir ../../packages/dist/wasm\" --",
    "build:wasm-release": "npm-run-all \"pack -- build ../crates/wasm --release --out-dir ../../packages/dist/wasm\" --",
    "build:node": "napi build --manifest-path ../../crates/node/Cargo.toml --platform -p node --release -o ./",
    "build:node-dev": "napi build --manifest-path ../../Cargo.toml --platform -p image-compress --js ./index.js --dts ./index.d.ts --target-dir ../../target -o ./",
    "prepack": "napi prepublish -p ./npm --tag-style npm",
    "artifacts": "napi artifacts --npm-dir ./npm",
    "version": "napi version --npm-dir ./npm",
    "pack": "wasm-pack"
  },
  "devDependencies": {
    "@napi-rs/cli": "^3.0.0-alpha.45",
    "@types/node": "^20.14.12",
    "npm-run-all": "^4.1.5"
  }
}
